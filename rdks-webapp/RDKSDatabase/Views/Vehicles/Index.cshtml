@model IEnumerable<RDKSDatabase.Models.Vehicle>

@{
    ViewData["Title"] = "Index";
}

<h1>Vehicles</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Customer)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DESCRIPTION)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BADGE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NOTES)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NOTES_2)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Customer.CUS_FNAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DESCRIPTION)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BADGE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NOTES)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NOTES_2)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LICENSE_PLATE">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LICENSE_PLATE">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LICENSE_PLATE">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
